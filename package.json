{
  "name": "twokinds",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development astro dev --port 4321 && npm run db:seed",
    "dev:prod": "cross-env NODE_ENV=production astro dev --verbose",
    "build": "cross-env NODE_ENV=production ASTRO_DATABASE_FILE=.astro/prod-db.sqlite astro build --no-remote",
    "build:local": "cross-env NODE_ENV=production ASTRO_PRODUCTION_DB_TYPE=local ASTRO_DATABASE_FILE=.astro/prod-db.sqlite astro build",
    "preview": "cross-env NODE_ENV=production astro preview",
    "start": "cross-env NODE_ENV=production ASTRO_DATABASE_FILE=.astro/prod-db.sqlite node ./dist/server/entry.mjs",
    "deploy": "cross-env NODE_ENV=production npm run build && node scripts/seed-prod-test.js && npm run start",
    "production:test": "rm -rf dist .astro/prod-db.sqlite && cross-env NODE_ENV=production ASTRO_PRODUCTION_DB_TYPE=local ASTRO_DATABASE_FILE=.astro/prod-db.sqlite astro build && node scripts/seed-prod-test.js && cross-env NODE_ENV=production ASTRO_DATABASE_FILE=.astro/prod-db.sqlite astro preview --port 4321",
    "astro": "astro",
    "db:push": "astro db push --local",
    "db:push:prod": "cross-env NODE_ENV=production astro db push --remote",
    "db:seed": "sqlite3 .astro/db.sqlite < seed.sql",
    "db:seed:prod": "cross-env NODE_ENV=production astro db execute db/seed.ts --remote",
    "setup": "npm run db:push && npm run db:seed",
    "seed-turso": "node seed-turso.mjs",
    "seed-remote": "cross-env NODE_ENV=production node seed-remote.mjs",
    "migrate:userid": "astro db execute db/migrate-user-id.ts",
    "lint": "eslint . --ext .ts,.astro",
    "lint:fix": "eslint . --ext .ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/db": "^0.17.1",
    "@astrojs/node": "^9.4.3",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^8.2.7",
    "@auth/core": "^0.37.4",
    "@hookform/resolvers": "^4.1.3",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.12",
    "astro": "^5.13.5",
    "auth-astro": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "lucide-react": "^0.479.0",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "shadcn-ui": "^0.9.5",
    "shareon": "^2.7.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "astro-eslint-parser": "^1.2.1",
    "better-sqlite3": "^11.8.1",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
